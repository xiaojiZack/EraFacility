:: testshowChara:Before
    
:: testshowChara
    <<showChara>>

:: showChara[widget]
    <<widget 'showChara'>>
        <<ShowCharaDiv>>
        <div id="SCbox1"  class="SCgridConter">
            <<showCharaBase>>
            <<ShowCharaPhoto>>
            <<ShowCharaLocation>>
            <<ShowCharaTrait>>
            <<ShowCharaApp>>
            <<ShowCharaEquip>>
        </div>
        <div id="SCbox2"  class="SCgridConter">
            <<ShowCharaSbl>>
            <<ShowCharaExp>>
            <<ShowCharaVirginity>>
        </div>
        <div id="SCbox3"  class="SCgridConter">
            <<ShowCharaBody>>
        </div>
        <div id="SCbox4" class="SCgridConter">
            <<ShowCharaMark>>
            <<ShowCharaPalam>>
        </div>
    <</widget>>

:: showCharaBase[widget]
    <<widget 'showCharaBase'>>
    <div id='SCbaseBox' class="SCBaseConter">
        <div id="SCname">
            $showChara.name
            <<script>>
                if (V.showChara.gender == "female") T.gendercoin = "♀";
                else{
                    if (V.showChara.gender == "male") T.gendercoin = "♂";
                    else T.gendercoin = "";}
            <</script>>
            &nbsp;_gendercoin
        </div>
        <<run F.initSCBaseBar()>>
        <div class="SCBaseBarConter">
            <<print T.printText.join('')>>
        </div>
    </div>
    <</widget>>

:: ShowCharaPhoto[widget]
    <<widget 'ShowCharaPhoto'>>
        <div class="SCphoto">
            
        </div>
    <</widget>>

:: ShowCharaTrait[widget]
    <<widget 'ShowCharaTrait'>>
        <div class='SCTrait'>
            <<run F.initSCTrait()>>
            <<print T.printText.join('')>>
        </div>
    <</widget>>

:: ShowCharaBody[widget]
    <<widget 'ShowCharaBody'>>
        <<if (V.showChara.flag.showBody || V.showChara.cid == "player")>>
            <div class='SCBody'>
                <<run F.initSCBody()>>
                <<print T.printText.join('')>>
            </div>
        <<else>>
            <div class='SCBody'>
            //////////<br>
                身体数据不明
            //////////<br>
            </div>
        <</if>>
    <</widget>>

:: ShowCharaVirginity[widget]
    <<widget 'ShowCharaVirginity'>>
        <div class='SCDashline'><<dashline>></div>
        <<run F.initSCVirginity()>>
        <<if (V.showChara.flag.showVirginity || V.showChara.cid == "player")>>
            <div>
                <<print T.printText.join('<br>')>>
            </div>
        <<else>>
            未知初次经验情况
        <</if>>
    <</widget>>

:: ShowCharaExp[widget]
    <<widget 'ShowCharaExp'>>
        <div class = 'SCDashline'><<dashline>></div>
        <<run F.initSCExp()>>
        <<if !V.showChara.flag.showExp and $showChara.cid !== "player">>
            <div class='SCExp-blur'>
                <<print T.printText.join('')>>
            </div>
        <<else>>
            <div class='SCExp'>
                <<print T.printText.join('')>>
            </div>
        <</if>>
    <</widget>>

:: ShowCharaSbl[widget]
    <<widget 'ShowCharaSbl'>>
        <div class = 'SCDashline'><<dashline>></div>
        <<run F.initSCSbl()>>
        <div class='SCSbl'>
