:: ShowCharaApp[widget]
    <<widget 'ShowCharaApp'>>
        <div class='SCAppBox'>
            <div>身高 <<=$showChara.appearance.height/10>>cm </div>
            <div>体重 <<=$showChara.appearance.weight>>kg </div>
            <div>胸围 <<=$showChara.appearance.bust/10>>cm</div>
            <div>[<<=D.breastSize[V.showChara.body.breasts.sizeLv]>>]</div>
            <div>腰围 <<=$showChara.appearance.waist/10>>cm</div>
            <div>臀围 <<=$showChara.appearance.hip/10>>cm</div>
            <div>肤色: <<=$showChara.appearance.skincolor>> </div>
            <div>瞳色: <<=$showChara.appearance.eyecolor>> </div>
            <div>发色: <<=$showChara.appearance.haircolor>> </div>
            <div>发型: <<=$showChara.appearance.hairstyle>> </div>
        </div>    
    <</widget>>

:: ShowCharaEquip[widget]
    <<widget 'ShowCharaEquip'>>
        <<if (V.showChara.flag.showEquip || V.showChara.cid == "player")>>
            <div class='SCDashline'><<dashline>></div>
            <<run F.initSCEquip()>>
            <div>
                <<print T.printText.join('<br>')>>
                <br>
                暴露值:<<=V.showChara.reveals.reveal>>
            </div>
        <<else>>
            <div class='SCDashline'><<dashline>></div>
            <div>
            <<run F.initVagueEquipDescribe()>>
            <<print T.printText.join("")>>
            <br>
            暴露值:<<=V.showChara.reveals.reveal>>
            </div>
        <</if>>
    <</widget>>

:: ShowCharaPalam[widget]
    <<widget 'ShowCharaPalam'>>
        <div class='SCDashline'><<dashline>></div>
        <div class="SCPalamBox">
            <div class="SCPalamtype">[心理]</div>
            <<if (V.showChara.flag.showPalam || V.showChara.cid == "player")>>
                <<run F.initSCPalam(D.mentalpalam)>>
                <<print T.printText.join('')>>
            <<else>>
                <<run F.generalPalamDescribe(D.mentalpalam)>>
                <<print T.printText.join('<br>')>>
            <</if>>
        </div>
        <div class="SCPalamBox">
            <<if (V.showChara.flag.showPalam || V.showChara.cid == "player")>>
                <div class="SCPalamtype">[快感]</div>
                <<run F.initSCPalam(D.ecstacypalam)>>
                <<print T.printText.join('')>>
            <</if>>
        </div>
        <div class="SCPalamBox">
            <<if (V.showChara.flag.showPalam || V.showChara.cid == "player")>>
                <div class="SCPalamtype">[痛楚]</div>
                <<run F.initSCPalam(D.painpalam)>>
                <<print T.printText.join('')>>
            <</if>>
        </div>
    <</widget>>

:: ShowCharaLocation[widget]
    <<widget 'ShowCharaLocation'>>
        <<if (Config.debug || V.showChara.cid !== "player")>>
        <div>
        当前位置:<<=worldMap[V.showChara.location.mapId].name[0]>>
        -
        <<run T.spotid = worldMap[V.showChara.location.mapId].findspot(V.showChara.location.coord[0],V.showChara.location.coord[1])>>
        <<= worldMap[V.showChara.location.mapId][T.spotid].name>>
        </div>
        <</if>>
    <</widget>>

