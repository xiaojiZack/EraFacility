:: ShowCharaDiv[widget]
    <<widget 'ShowCharaDiv'>>
        <<script>>
            const allpages = ['SCbox1','SCbox2','SCbox3'];
            function SCchangeDiv(pagename){
                var page = document.getElementById(pagename);
                for (let i of allpages){
                    var hidepage = document.getElementById(i);
                    hidepage.style.display = "none";
                }
                page.style.display = "grid";
            }
        <</script>>
        <div class="SCDivButton">
            <button id="Divbutton1" style='color:darkmagenta'  onclick="F.SCchangeDiv('1')">基本信息</button>
            <button id="Divbutton2"  onclick="F.SCchangeDiv('2')">调教开发</button>
            <button id="Divbutton3"  onclick="F.SCchangeDiv('3')">肉体信息</button>
            <button id="Divbutton4"  onclick="F.SCchangeDiv('4')">心理状态</button>
        </div>
    <</widget>>

